import java.util.Scanner;
class Voting{
public static void main(String []args){

System.out.println("ENTER YOUR AGE :");
Scanner st = new Scanner(System.in);
int age = st.nextInt();

if(age>18)
{
System.out.println("your age is: "+age+ " now yooooou can vote");
}
else
{
System.out.println("your age is: "+age+" pls try next time");
}

}
}
